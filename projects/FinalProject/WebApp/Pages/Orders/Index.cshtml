@page
@model WebApp.Pages.Orders.IndexModel

@{
    ViewData["Title"] = "Заказы";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrderInfo[0].OrderId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderInfo[0].User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderInfo[0].OrderDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderInfo[0].TotalPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderInfo[0].ShoeArticles)
            </th>
        </tr>
    </thead>
    <tbody>
 @foreach (var item in Model.OrderInfo)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OrderId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShoeArticles)
        </td>
    </tr>
}
    </tbody>
</table>
